Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

! URxvt settings

URxvt.depth:                32
URxvt.transparent:          false
URxvt.saveLines:            50
URxvt.internalBorder:       1
URxvt.lineSpace:            0

! Fonts
URxvt.allow_bold:           true
URxvt*font:                 -*-clean-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt*boldFont:             -*-clean-bold-r-*-*-12-*-*-*-*-*-*-*
!URxvt*font:                 xft:Inconsolata:pixelsize=11
!URxvt*boldFont:             xft:Inconsolata Bold:pixelsize=11

URxvt*letterSpace: 0

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

! Source http://github.com/altercation/solarized

*background: #002b36
*foreground: #657b83
*fadeColor: #002b36
*cursorColor: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3
